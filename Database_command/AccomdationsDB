{
   validator: {
      $jsonSchema: {
        bsonType: 'object',
        required: [
          'name',
          'description',
          'address',
          'rating',
          'category',
          'rooms',
          'add_by_an_user'
        ],
        properties: {
          name: {
            bsonType: 'string',
            description: 'Name of the hotel'
          },
          description: {
            bsonType: 'string',
            description: 'Description of the hotel'
          },
          address: {
            bsonType: 'object',
            required: [
              'number',
              'street',
              'city',
              'country'
            ],
            properties: {
              number: {
                bsonType: 'number',
                minimum: 0,
                description: 'Hotel number'
              },
              street: {
                bsonType: 'string',
                description: 'Hotel street'
              },
              city: {
                bsonType: 'string',
                description: 'Hotel city'
              },
              country: {
                bsonType: 'string',
                description: 'Hotel country'
              }
            }
          },
          rating: {
            bsonType: 'array',
            items: {
              bsonType: 'object',
              required: [
                'rating',
                'comment',
                'author'
              ],
              properties: {
                rating: {
                  bsonType: 'number',
                  minimum: 0,
                  maximum: 5,
                  description: 'Hotel rating on a scale of 0 to 5'
                },
                comment: {
                  bsonType: 'string',
                  description: 'User\'s notice'
                },
                author: {
                    bsonType: 'string',
                    description: 'author'
                }
              }
            }
          },
          category: {
            bsonType: 'string',
            description: 'Hotel category'
          },
          amenities: {
            bsonType: 'array',
            items: {
              bsonType: 'string',
              description: 'Amenities offered by the hotel'
            }
          },
          activities: {
            bsonType: 'array',
            items: {
              bsonType: 'string',
              description: 'Activities near the accommodation'
            }
          },
          rooms: {
            bsonType: 'array',
            items: {
              bsonType: 'object',
              required: [
                'type',
                'price',
                'dates',
                'numberOfPax'
              ],
              properties: {
                type: {
                  bsonType: 'string',
                  description: 'Hotel room type'
                },
                price: {
                  bsonType: 'number',
                  description: 'Room price'
                },
                dates: {
                  bsonType: 'array',
                  items: {
                    bsonType: 'date',
                    description: 'Dates when the room is available'
                  }
                },
                numberOfPax: {
                    bsonType: 'number',
                    description: 'Number of pax'
                },
                photos: {
                    bsonType: 'array',
                    items: {
                        bsonType: 'string',
                        description: 'Room photos'
                    }
                }
              }
            }
          },
          contact: {
            bsonType: 'object',
            properties: {
                phone: {
                    bsonType: 'string',
                    description: 'Accommodation phone number'
                },
                email: {
                    bsonType: 'string',
                    description: 'Accommodation email'
                },
                website: {
                    bsonType: 'string',
                    description: 'Accommodation website'
                }
            }
          },
          add_by_an_user: {
              bsonType: 'bool',
              description: 'Accommodation added by an user'
          },
          photos: {
              bsonType: 'array',
              items: {
                  bsonType: 'string',
                  description: 'Accommodation photos'
              }
          }
        }
      }
   }
}



