              country: {
                bsonType: 'string',
                description: 'Hotel country'
              }
            }
          },
          rating: {
            bsonType: 'array',
            items: {
              bsonType: 'object',
              required: [
                'rating',
                'comment',
                'author'
              ],
              properties: {
                rating: {
                  bsonType: 'number',
                  minimum: 0,
                  maximum: 5,
                  description: 'Hotel rating on a scale of 0 to 5'
                },
                comment: {
                  bsonType: 'string',
                  description: 'User\'s notice'
                },
                author: {
                    bsonType: 'string',
                    description: 'author'
                }
              }
            }
          },
          category: {
            bsonType: 'string',
            description: 'Hotel category'
          },
