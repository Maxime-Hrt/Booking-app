              bsonType: 'string',
              description: 'Activities near the accommodation'
            }
          },
          rooms: {
            bsonType: 'array',
            items: {
              bsonType: 'object',
              required: [
                'type',
                'price',
                'dates',
                'numberOfPax'
              ],
              properties: {
                type: {
                  bsonType: 'string',
                  description: 'Hotel room type'
                },
                price: {
                  bsonType: 'number',
                  description: 'Room price'
                },
                dates: {
                  bsonType: 'array',
                  items: {
                    bsonType: 'date',
                    description: 'Dates when the room is available'
                  }
                },
                numberOfPax: {
                    bsonType: 'number',
                    description: 'Number of pax'
                },
                photos: {
                    bsonType: 'array',
                    items: {
                        bsonType: 'string',
                        description: 'Room photos'
                    }
                }
              }
            }
          },
          contact: {
            bsonType: 'object',
            properties: {
                phone: {
                    bsonType: 'string',
                    description: 'Accommodation phone number'
                },
                email: {
                    bsonType: 'string',
                    description: 'Accommodation email'
                },
                website: {
                    bsonType: 'string',
                    description: 'Accommodation website'
                }
            }
          },
          add_by_an_user: {
              bsonType: 'bool',
              description: 'Accommodation added by an user'
          },
          photos: {
              bsonType: 'array',
              items: {
                  bsonType: 'string',
                  description: 'Accommodation photos'
              }
          }
        }
      }
   }
